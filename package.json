{
  "name": "skecthnow",
  "version": "1.0.0",
  "main": "index.ts",
  "author": "Anoop",
  "license": "MIT",
  "scripts": {
    "dev": "nodemon index.ts",
    "start": "node dist/index.js",
    "build": "npx tsc"
  },
  "dependencies": {
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "mongodb": "^6.5.0",
    "mongoose": "^8.2.4"
  },
  "devDependencies": {
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.17",
    "@types/node": "^20.5.1",
    "concurrently": "^8.2.0",
    "eslint": "^8.57.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.1.3",
    "nodemon": "^3.0.1",
    "prettier": "^3.2.5",
    "ts-node-dev": "^2.0.0",
    "typescript": "^5.1.6"
  },
  "eslintConfig": {
    "parserOptions": {
      "ecmaVersion": 6
    },
    "extends": [
      "eslint:recommended",
      "plugin:prettier/recommended"
    ],
    "plugins": [
      "prettier"
    ],
    "env": {
      "node": true,
      "es6": true
    },
    "rules": {
      "semi": "error",
      "quotes": [
        "error",
        "double"
      ]
    }
  },
  "prettier": {
    "semi": true,
    "singleQuote": false
  }
}
